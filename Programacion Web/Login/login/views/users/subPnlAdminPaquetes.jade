                  div(class='form-group' )
                    label(for='nombreP')='Identificador Paquete'
                    div(class='row')
                      div(class='col-md-4')
                        input(type='text', name='nombrePaquete', placeholder='Nombre del Paquete', class='form-control')
                      div(class='col-md-8')
                        div(class='form-group' )
                          select.form-control
                            option Gold
                            option Premium
                            option Basico
                            option Turista
                            option Familiar
